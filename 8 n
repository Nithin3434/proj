select year,avg(prevailing_wage) as average from h1b_final where year=2011 and full_time_position='N' and case_status='CERTIFIED-WITHDRAWN' group by year,job_title order by average desc;

select year,avg(prevailing_wage) as average from h1b_final where year=2012 and full_time_position='N' and case_status='CERTIFIED-WITHDRAWN' group by year,job_title order by average desc;

select year,avg(prevailing_wage) as average from h1b_final where year=2013 and full_time_position='N' and case_status='CERTIFIED-WITHDRAWN' group by year,job_title order by average desc;

select year,avg(prevailing_wage) as average from h1b_final where year=2014 and full_time_position='N' and case_status='CERTIFIED-WITHDRAWN' group by year,job_title order by average desc;

select year,avg(prevailing_wage) as average from h1b_final where year=2015 and full_time_position='N' and case_status='CERTIFIED-WITHDRAWN' group by year,job_title order by average desc;

select year,avg(prevailing_wage) as average from h1b_final where year=2016 and full_time_position='N' and case_status='CERTIFIED-WITHDRAWN' group by year,job_title order by average desc;
